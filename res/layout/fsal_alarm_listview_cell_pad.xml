<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/FSAl_Alarm_ListView_CellBG_RLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#00000000"
    >
    <RelativeLayout 
        android:id="@+id/FSAl_Alarm_ListView_Cell_RLayout"
        android:layout_width="match_parent"
    	android:layout_height="match_parent" 
    	android:background="#00000000"
        >
        <ImageView 
            android:id="@+id/FSAl_Alarm_ListView_Cell_RLayout_Delete_ImageView"
		    android:layout_width="wrap_content"
		   	android:layout_height="wrap_content"
		   	android:layout_alignParentLeft="true"
		   	android:layout_centerVertical="true"
		   	android:scaleType="fitXY"
		   	android:background="#ff0000"
            />
        
        <TextView 
	    	android:id="@+id/FSAl_Alarm_ListView_Cell_RLayout_Time_TextView"
		    android:layout_width="wrap_content"
		   	android:layout_height="wrap_content"
		   	android:layout_alignWithParentIfMissing="true"
		   	android:layout_toRightOf="@+id/FSAl_Alarm_ListView_Cell_RLayout_Delete_ImageView"
		   	android:layout_alignParentTop="true"
		   	android:singleLine="true"
		   	android:gravity="center_vertical"
		   	android:padding="0dp"	        	
	        android:textColor="#ffffff"	
	        android:background="#0000ff"	   	  		
	       	/>
	    <TextView 
	       	android:id="@+id/FSAl_Alarm_ListView_Cell_RLayout_Frequency_TextView"
		    android:layout_width="wrap_content"
		   	android:layout_height="match_parent"
		   	android:layout_alignWithParentIfMissing="true"
		   	android:layout_toRightOf="@+id/FSAl_Alarm_ListView_Cell_RLayout_Delete_ImageView"
		   	android:layout_below="@+id/FSAl_Alarm_ListView_Cell_RLayout_Time_TextView"
		   	android:singleLine="true"
		   	android:gravity="center_vertical"
		   	android:padding="0dp"			   	  	
	        android:textColor="#ffffff"
	        android:background="#00ff00"		   	
	       	/>
	    <TextView 
	       	android:id="@+id/FSAl_Alarm_ListView_Cell_RLayout_AlarmName_TextView"
		    android:layout_width="wrap_content"
		   	android:layout_height="match_parent"
		   	android:layout_toLeftOf="@+id/FSAl_Alarm_ListView_Cell_RLayout_Arrow_ImageView"
		   	android:singleLine="true"
		   	android:gravity="center_vertical"
		   	android:layout_centerVertical="true"
		   	android:padding="0dp"	        	
	        android:textColor="#ffffff"	
	        android:background="#ffff00"	   	
	       	/>
	    <ImageView 
            android:id="@+id/FSAl_Alarm_ListView_Cell_RLayout_Arrow_ImageView"
		    android:layout_width="wrap_content"
		   	android:layout_height="wrap_content"
		   	android:layout_alignParentRight="true"
		   	android:layout_centerVertical="true"
		   	android:scaleType="fitXY"
		   	android:background="#ff00ff"		   	
            />
    </RelativeLayout>
</RelativeLayout>
