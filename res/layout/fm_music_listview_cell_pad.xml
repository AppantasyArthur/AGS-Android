<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/FM_Music_ListView_CellBG_RLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#000000"
    >
    <RelativeLayout 
        android:id="@+id/FM_Music_ListView_Cell_RLayout"
        android:layout_width="match_parent"
    	android:layout_height="match_parent"
        >
        <TextView 
	        android:id="@+id/FM_Music_ListView_Cell_RLayout_Name_TextView"
	        android:layout_width="match_parent"
	    	android:layout_height="wrap_content"
	    	android:layout_alignParentLeft="true"
	    	android:layout_alignParentTop="true"
	    	android:layout_toLeftOf="@+id/FM_Music_ListView_Cell_RLayout_Image_ImageView"
	    	android:gravity="center_vertical"
	    	android:padding="0dp"
	    	android:textColor="#ffffff"
	    	android:text="text"	    	
        />
        <ImageView 
            android:id="@+id/FM_Music_ListView_Cell_RLayout_Image_ImageView"
       		android:layout_width="wrap_content"
    		android:layout_height="wrap_content"
    		android:layout_alignParentRight="true"
    		android:layout_alignParentTop="true"
    		android:background="#00000000"  		
            />
    </RelativeLayout>
</RelativeLayout>
