<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pFAS_RLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ViewFlipper 
        android:id="@+id/pFAS_RLayout_ViewContent_ViewFlipper"
	    android:layout_width="match_parent"
	   	android:layout_height="match_parent"
        >
		<FrameLayout 
		    android:id="@+id/pFAS_RLayout_ViewFlipper_Left_FLayout"
	    	android:layout_width="match_parent"
	   		android:layout_height="match_parent"	   		
	   		android:background="#000000"
		    >  
		</FrameLayout>
		<FrameLayout 
		    android:id="@+id/pFAS_RLayout_ViewFlipper_Right_FLayout"
	    	android:layout_width="match_parent"
	   		android:layout_height="match_parent"	   		
	   		android:background="#000000"
		    >  
		</FrameLayout>
	</ViewFlipper>
</RelativeLayout>
