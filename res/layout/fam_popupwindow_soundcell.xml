<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/FAM_PopupWindow_SoundCell_RLayout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="#00000000"
    android:clickable="false"
    >
  	<ImageView 
  	    android:id="@+id/FAM_PopupWindow_SoundCell_RLayout_SOUND_IView"
  	  	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_centerVertical="true"
    	android:layout_alignParentLeft="true"
    	android:background="#00000000"
  	    />
  	<SeekBar 
  	    android:id="@+id/FAM_PopupWindow_SoundCell_RLayout_SOUND_SeekBar"
  	  	android:layout_width="wrap_content"
    	android:layout_height="wrap_content"
    	android:layout_centerVertical="true"
    	android:layout_toRightOf="@+id/FAM_PopupWindow_SoundCell_RLayout_SOUND_IView"    	
		android:max="100"
		android:progressDrawable="@drawable/seekbar_sort_style"
		android:thumbOffset="0dp"
  	    />
</RelativeLayout>
