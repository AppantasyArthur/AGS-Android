<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/FSR_RLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    >
	<RelativeLayout 
	    android:id="@+id/FSR_RLayout_TITLE_RLayout"
    	android:layout_width="match_parent"
   		android:layout_height="wrap_content"
   		android:layout_alignParentTop="true"
   		android:background="#00000000"
	    >  
	    <TextView 
            android:id="@+id/FSR_RLayout_RLayout_Title_TextView"
        	android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:layout_centerInParent="true"
        	android:padding="0dp"
        	android:textStyle="bold"
        	android:textColor="#ff0000"
        	android:gravity="center"
        	android:text="Speakers"
            />
	</RelativeLayout>
	<RelativeLayout 
	    android:id="@+id/FSR_RLayout_BODY_RLayout"
    	android:layout_width="match_parent"
   		android:layout_height="match_parent"
   		android:layout_below="@+id/FSR_RLayout_TITLE_RLayout"
   		android:background="#00000000"
	    >
	    <RelativeLayout 
		    android:id="@+id/FSR_RLayout_RLayout_Renders_RLayout"
	    	android:layout_width="wrap_content"
	   		android:layout_height="wrap_content"
	   		android:layout_alignParentTop="true"
	   		android:layout_alignParentLeft="true"
	   		android:background="#00000000"
		    >  
		    <ListView 
		        android:id="@+id/FSR_RLayout_RLayout_Renders_ListView"
		        android:layout_width="match_parent"
	   			android:layout_height="match_parent"	   			
	   			android:divider="#000000"
	   			android:background="#00000000"
	       		android:listSelector="@android:color/transparent"
		        >
		    </ListView>
		</RelativeLayout>
	</RelativeLayout>
</RelativeLayout>
