<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pFSAl_List_RLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    >
	<RelativeLayout 
	    android:id="@+id/pFSAl_List_RLayout_TITLE_RLayout"
    	android:layout_width="match_parent"
   		android:layout_height="wrap_content"
   		android:layout_alignParentTop="true"
   		android:background="#00000000"
	    > 
	    <ImageView 
	        android:id="@+id/pFSAl_List_RLayout_RLayout_TITLEBG_ImageView"
	    	android:layout_width="match_parent"
	   		android:layout_height="match_parent"
	   		android:layout_alignParentRight="true"
	   		android:scaleType="fitXY"
	   		android:background="#00000000"
	        /> 
	    <Button
            android:id="@+id/pFSAl_List_RLayout_RLayout_Back_Button"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_alignParentLeft="true"
        	android:layout_centerVertical="true"
        	android:padding="0dp"        	
        	android:textColor="#ffffff"
        	android:gravity="center"
        	android:text="Back"
            /> 
	    <Button
            android:id="@+id/pFSAl_List_RLayout_RLayout_Edit_Button"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_toRightOf="@+id/pFSAl_List_RLayout_RLayout_Back_Button"
        	android:layout_centerVertical="true"
        	android:padding="0dp"        	
        	android:textColor="#ffffff"
        	android:gravity="center"
        	android:text="Edit"
            />
	           
	    <TextView 
            android:id="@+id/pFSAl_List_RLayout_RLayout_Title_TextView"
        	android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:layout_centerInParent="true"
        	android:padding="0dp"
        	android:textStyle="bold"
        	android:textColor="#ff0000"
        	android:gravity="center"
        	android:text="Alarm"
            />
	    <Button
            android:id="@+id/pFSAl_List_RLayout_RLayout_Add_Button"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_alignParentRight="true"
        	android:layout_centerVertical="true"
        	android:padding="0dp"        	
        	android:textColor="#ffffff"
        	android:gravity="center"        	
            />
	</RelativeLayout>
	<RelativeLayout 
	    android:id="@+id/pFSAl_List_RLayout_BODY_RLayout"
    	android:layout_width="match_parent"
   		android:layout_height="match_parent"
   		android:layout_below="@+id/pFSAl_List_RLayout_TITLE_RLayout"
   		android:background="#00000000"
	    > 
	    <RelativeLayout 
	        android:id="@+id/pFSAl_List_RLayout_RLayout_Alarm_RLayout"
	    	android:layout_width="wrap_content"
	   		android:layout_height="wrap_content"
	   		android:layout_alignParentTop="true"
		   	android:layout_alignParentLeft="true"
	   		android:background="#00000000"
	        >
		    <ListView 
		        android:id="@+id/pFSAl_List_RLayout_RLayout_Alarm_ListView"
		    	android:layout_width="match_parent"
		   		android:layout_height="match_parent"
		   		android:listSelector="@android:color/transparent"
		   		android:divider="#00000000"
		   		android:background="#00000000"
		        >
		    </ListView>
	    </RelativeLayout> 
	</RelativeLayout>
</RelativeLayout>
