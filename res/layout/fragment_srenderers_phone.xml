<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pFSR_RLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ff0000"
    >
	<RelativeLayout 
	    android:id="@+id/pFSR_RLayout_TITLE_RLayout"
    	android:layout_width="match_parent"
   		android:layout_height="wrap_content"
   		android:layout_alignParentTop="true"
   		android:background="#474747"
	    >  
	    <Button
            android:id="@+id/pFSR_RLayout_RLayout_Back_Button"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:layout_alignParentLeft="true"
        	android:layout_centerVertical="true"
        	android:padding="0dp"
        	android:textStyle="bold"
        	android:textColor="#ffffff"
        	android:gravity="center"
        	android:text="Back"
            />    
	    <TextView 
            android:id="@+id/pFSR_RLayout_RLayout_Title_TextView"
        	android:layout_width="match_parent"
        	android:layout_height="wrap_content"        	
        	android:layout_alignParentTop="true"
        	android:layout_centerHorizontal="true"
        	android:padding="0dp"
        	android:textStyle="bold"
        	android:textColor="#ff0000"
        	android:gravity="center"
        	android:text="Settings"
            />
	    <TextView 
            android:id="@+id/pFSR_RLayout_RLayout_Title2_TextView"
        	android:layout_width="match_parent"
        	android:layout_height="wrap_content"
        	android:layout_below="@+id/pFSR_RLayout_RLayout_Title_TextView"
        	android:layout_centerHorizontal="true"
        	android:padding="0dp"
        	android:textStyle="bold"
        	android:textColor="#bdbdbd"
        	android:gravity="center"
        	android:text="Speakers"
            />
	</RelativeLayout>
	<RelativeLayout 
	    android:id="@+id/pFSR_RLayout_BODY_RLayout"
    	android:layout_width="match_parent"
   		android:layout_height="match_parent"
   		android:layout_below="@+id/pFSR_RLayout_TITLE_RLayout"
   		android:background="#373737"
	    > 
	    <RelativeLayout 
		    android:id="@+id/pFSR_RLayout_RLayout_Renders_RLayout"
	    	android:layout_width="match_parent"
	   		android:layout_height="match_parent"
	   		android:layout_alignParentTop="true"
	   		android:layout_centerHorizontal="true"
	   		android:background="#00000000"
		    > 
		    <ListView 
		        android:id="@+id/pFSR_RLayout_RLayout_Renders_ListView"
		        android:layout_width="match_parent"
	   			android:layout_height="match_parent"	   			
	   			android:divider="#000000"
	   			android:background="#00000000"
	       		android:listSelector="@android:color/transparent"
		        >
		    </ListView>
		</RelativeLayout>
	</RelativeLayout>
</RelativeLayout>
