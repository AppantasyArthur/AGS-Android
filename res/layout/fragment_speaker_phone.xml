<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/pFS_RLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000" >
    <RelativeLayout 
        android:id="@+id/pFS_RLayout_TITLE2_RLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:background="#00000000"
        >   
        <Button 
            android:id="@+id/pFS_RLayout_RLayout_Nowplaying_Button"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_centerVertical="true"
	        android:layout_alignParentLeft="true"	        
	        android:background="#00000000"
	        android:gravity="center"
	        android:textColor="#ffffff"	        
	        android:padding="0dp"
	        android:text="Now playing"
            />    
        <TextView
            android:id="@+id/pFS_RLayout_RLayout_Center_TextView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:textStyle="bold"
            android:padding="0dp"
            android:gravity="center"
            android:textColor="#ffffff"
            android:text="Speakers"      
            />
        <Button 
            android:id="@+id/pFS_RLayout_RLayout_Music_Button"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_centerVertical="true"
	        android:layout_alignParentRight="true"
	        android:background="#00000000"
	        android:gravity="center"
	        android:textColor="#ffffff"	        
	        android:padding="0dp"
	        android:text="Music"
            />    
    </RelativeLayout>
    <RelativeLayout 
        android:id="@+id/pFS_RLayout_TITLE2_1_RLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:visibility="gone"
        android:background="#00000000"
        >   
        <Button 
            android:id="@+id/pFS_RLayout_RLayout_Close_Button"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_centerVertical="true"
	        android:layout_alignParentLeft="true"	        
	        android:background="#00000000"
	        android:gravity="center"
	        android:textColor="#ffffff"	        
	        android:padding="0dp"
	        android:text="Close"
            />    
        <TextView
            android:id="@+id/pFS_RLayout_RLayout_Center2_TextView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_centerInParent="true"
            android:textStyle="bold"
            android:padding="0dp"
            android:gravity="center"
            android:textColor="#ffffff"
            android:text="Group Rooms"      
            />
        <Button 
            android:id="@+id/pFS_RLayout_RLayout_Done_Button"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_centerVertical="true"
	        android:layout_alignParentRight="true"
	        android:background="#00000000"
	        android:gravity="center"
	        android:textColor="#ffffff"	        
	        android:padding="0dp"
	        android:text="Done"
            />    
    </RelativeLayout>
    <RelativeLayout 
		android:id="@+id/pFS_RLayout_TITLE3_RLayout"
		android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/pFS_RLayout_TITLE2_RLayout"
	    android:background="#00000000"
		>
		<ImageButton 
		    android:id="@+id/pFS_RLayout_RLayout_Sound_IButton"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:scaleType="fitXY"
		    android:background="#00000000"
		    />
		<SeekBar
		    android:id="@+id/pFS_RLayout_RLayout_Sound_SeekBar"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_toRightOf="@+id/pFS_RLayout_RLayout_Sound_IButton"
		    android:layout_centerVertical="true"	        	   
		    android:max="100"
		    android:progressDrawable="@drawable/seekbar_sort_style"		        
		    android:thumbOffset="0dp"
		    />	        	        
		<ImageButton 
		    android:id="@+id/pFS_RLayout_RLayout_Previous_IButton"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:scaleType="fitXY"
		    android:background="#00000000"		       
		    />
		<ImageButton 
		    android:id="@+id/pFS_RLayout_RLayout_Play_IButton"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:scaleType="fitXY"
		    android:background="#00000000"
		    />
		<ImageButton 
		    android:id="@+id/pFS_RLayout_RLayout_Next_IButton"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:scaleType="fitXY"
		    android:background="#00000000"
		    />
		<ImageButton 
		    android:id="@+id/pFS_RLayout_RLayout_ShowTITLE4_IButton"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentRight="true"
		    android:layout_centerVertical="true"
		    android:scaleType="fitXY"
		    android:background="#00000000"
		    />
	</RelativeLayout>
	<com.FAM.SETTING.FAM_ViewFlipper 
	    android:id="@+id/pFS_RLayout_ViewContent_ViewFlipper"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"       
	    android:layout_below="@+id/pFS_RLayout_TITLE3_RLayout"
	    android:layout_above="@+id/pFS_RLayout_Bottom_RLayout"
	    android:background="#00000000"  
	    > 	
	    <RelativeLayout 
	        android:id="@+id/up"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        >       
			<ExpandableListView
		        android:id="@+id/pFS_RLayout_SPEAKER_EListView"
		        android:layout_width="match_parent"
		        android:layout_height="match_parent"
		        android:listSelector="@android:color/transparent"	           
		        android:background="#00000000"
		        android:divider="#00000000"    
		        >        
		    </ExpandableListView>	  
		</RelativeLayout>	
		<RelativeLayout 
	        android:id="@+id/down"
	        android:layout_width="match_parent"
	        android:layout_height="match_parent"
	        >  
		    <ScrollView
	            android:id="@+id/pFS_RLayout_ViewFlipper_ChooseScroll_ScrollView"
	            android:layout_width="match_parent"
			    android:layout_height="match_parent"			   		    
			    android:background="#00000000"		    
	            >
	            <LinearLayout
	                android:id="@+id/pFS_RLayout_ViewFlipper_ScrollView_ChooseScroll_LLayout"
		            android:layout_width="match_parent"
				    android:layout_height="wrap_content"
				    android:orientation="vertical"
				    ></LinearLayout>	    	
        	</ScrollView> 
        </RelativeLayout>           
    </com.FAM.SETTING.FAM_ViewFlipper>	  
    <RelativeLayout 
	    android:id="@+id/pFS_RLayout_TITLE4_RLayout"
		android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:layout_below="@+id/pFS_RLayout_TITLE3_RLayout"	    
	    >
		<ImageButton 
			android:id="@+id/pFS_RLayout_RLayout_Cycle_IButton"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_alignParentTop="true"
		    android:scaleType="fitXY"
		    android:background="#00000000"
		    />		   
		<ImageButton 
		    android:id="@+id/pFS_RLayout_RLayout_Random_IButton"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_toRightOf="@+id/pFS_RLayout_RLayout_Cycle_IButton"		        
		    android:layout_alignParentTop="true"
		    android:scaleType="fitXY"
		    android:background="#00000000"
		    />		     
		<TextView 
		    android:id="@+id/pFS_RLayout_RLayout_Current_TextView"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:padding="0dp"	
		    android:gravity="center"
		    android:background="#00000000"			        
		    android:text="00:00"        
		    />
		<SeekBar 
		    android:id="@+id/pFS_RLayout_RLayout_Music_SeekBar"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_toLeftOf="@+id/pFS_RLayout_RLayout_Total_TextView"
		    android:layout_toRightOf="@+id/pFS_RLayout_RLayout_Current_TextView"
		    android:progressDrawable="@drawable/seekbar_style"		        
		    android:thumbOffset="0dp"		        
		    android:background="#00000000"		       
		    />
		<TextView 		    
		    android:id="@+id/pFS_RLayout_RLayout_Total_TextView"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentRight="true"
		    android:layout_centerVertical="true"
		    android:padding="0dp"
		    android:gravity="center"	
		    android:background="#00000000"		         
		    android:text="-00:00"      	
		    />
	</RelativeLayout>	
    <RelativeLayout 
        android:id="@+id/pFS_RLayout_Bottom_RLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        >
       	<ImageButton 
       	    android:id="@+id/pFS_RLayout_RLayout_Setting_IButton"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:background="#00000000"
	        android:scaleType="fitXY"
       	    />
       	<Button 
       	    android:id="@+id/pFS_RLayout_RLayout_PASUEALL_Button"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"	        
	        android:layout_centerInParent="true"
	        android:background="#00000000"
	        android:scaleType="fitXY"
	        android:padding="0dp"
		    android:textColor="#ffffff"
		    android:text="Pasue ALL"
		    android:textStyle="bold"
       	    />
    </RelativeLayout> 
    <RelativeLayout 
        android:id="@+id/pFS_RLayout_Bottom2_RLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:visibility="gone"
        >
       	<ImageButton 
       	    android:id="@+id/pFS_RLayout_RLayout_Setting2_IButton"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:background="#00000000"
	        android:scaleType="fitXY"
       	    />
       	<Button 
       	    android:id="@+id/pFS_RLayout_RLayout_SELECT_Button"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"	        
	        android:layout_centerInParent="true"
	        android:background="#00000000"
	        android:scaleType="fitXY"
	        android:padding="0dp"
		    android:textColor="#ffffff"
		    android:text="Select"
		    android:textStyle="bold"
       	    />
    </RelativeLayout>   
</RelativeLayout>
