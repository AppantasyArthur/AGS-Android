<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/FAS_RLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
	<ImageView 
	    android:id="@+id/FAS_RLayout_Right_ImageView"
    	android:layout_width="wrap_content"
   		android:layout_height="match_parent"
   		android:layout_alignParentRight="true"
   		android:background="#00000000"
   		android:scaleType="fitXY"
	    />
	<ImageView 
	    android:id="@+id/FAS_RLayout_Left_ImageView"
    	android:layout_width="wrap_content"
   		android:layout_height="match_parent"   		
   		android:layout_alignParentLeft="true"
   		android:background="#00000000"
   		android:scaleType="fitXY"
	    />
	<ImageView
	    android:id="@+id/FAS_RLayout_TitleBG_ImageView"
    	android:layout_width="match_parent"
   		android:layout_height="wrap_content"
   		android:layout_alignParentTop="true"
   		android:background="#00000000"
   		android:scaleType="fitXY"
	    />
    
	<RelativeLayout 
	    android:id="@+id/FAS_RLayout_Right_RLayout"
    	android:layout_width="wrap_content"
   		android:layout_height="match_parent"
   		android:layout_alignParentRight="true"
   		android:background="#00000000"
	    >  
	</RelativeLayout>
	<RelativeLayout 
	    android:id="@+id/FAS_RLayout_Left_RLayout"
    	android:layout_width="wrap_content"
   		android:layout_height="match_parent"
   		android:layout_alignParentLeft="true"
   		android:background="#00000000"
	    >  
	</RelativeLayout>
</RelativeLayout>
